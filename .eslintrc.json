{
    "globals": {
        "By": "readonly",
        "shared": "writable",
        "page": "writable",
        "driver": "readonly",
        "expect": "readonly",
        "until": "writable",
        "helpers": "writable",
        "by": "readonly",
        "DEFAULT_TIMEOUT": "writable",
        "browserName": "writable",
        "eyes": "writable"
    },
    "root": true,
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "mocha": true,
        "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 9
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
        "array-bracket-spacing": [
            "warn",
            "never"
        ],
        "arrow-body-style": [
            "warn",
            "as-needed"
        ],
        "arrow-parens": [
            "warn",
            "as-needed"
        ],
        "arrow-spacing": "warn",
        "brace-style": [
            "warn",
            "1tbs"
        ],
        "camelcase": "warn",
        "comma-spacing": [
            "warn",
            {
                "after": true
            }
        ],
        "dot-notation": "warn",
        "eqeqeq": [
            "warn",
            "smart"
        ],
        "indent": [
            "warn",
            4,
            {
                "SwitchCase": 1,
                "FunctionDeclaration": {
                    "parameters": 1
                },
                "MemberExpression": 1,
                "CallExpression": {
                    "arguments": 1
                }
            }
        ],
        "key-spacing": [
            "warn",
            {
                "beforeColon": false,
                "afterColon": true,
                "mode": "minimum"
            }
        ],
        "keyword-spacing": "warn",
        "no-console": "off",
        "no-empty": "off",
        "no-multi-spaces": "warn",
        "no-redeclare": "off",
        "no-restricted-globals": [
            "warn",
            "Promise"
        ],
        "no-trailing-spaces": "warn",
        "no-undef": "error",
        "no-unused-vars": [
            "warn",
            {
                "args": "none"
            }
        ],
        "one-var": [
            "warn",
            "never"
        ],
        "padded-blocks": [
            "warn",
            "never"
        ],
        "object-curly-spacing": [
            "warn",
            "never"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "react/prop-types": "off",
        "react/jsx-no-bind": "off",
        "semi": [
            "warn",
            "always"
        ],
        "space-before-blocks": [
            "warn",
            "always"
        ],
        "space-before-function-paren": [
            "off",
            "never"
        ],
        "space-in-parens": [
            "warn",
            "never"
        ]
    }
}